{
    "name": "start-of-day",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "build": "ng build",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "build:allW": "concurrently \"ng build --watch welcome\" \"ng build --watch clients\" \"ng build --watch news\" \"ng build --watch portfolio\" \"ng build --watch transactions\"",
        "build:allP": "concurrently \"ng build --prod welcome\" \"ng build --prod clients\" \"ng build --prod news\" \"ng build --prod portfolio\" \"ng build --prod transactions\"",
        "build:prod": "npm run build:allP && node build.js",
        "start": "npm run build:prod && npm run serve",
        "serve": "http-server -- ./dist/"
    },
    "private": true,
